image{width: 100%;height: 100%}
.tab{font-size: 28rpx;color: #999999;background: #ffffff;overflow: hidden;position: relative;}
.tab_item{width: 33.33%;text-align: center;line-height: 80rpx;float: left;}
.tabBar_bottom{ border-bottom: 5rpx solid  #e6bf6b;transition: 0.5s;position: absolute;bottom: 0;left: 0;width: 33%}
.tabBar_item_active{color:#e6bf6b;}
.warp{background: #f3f3f3;padding: 20rpx 25rpx;min-height: 1180rpx;}
.item{overflow: hidden;margin-bottom: 20rpx;width: 700rpx;height: 200rpx;position: relative}
.item_val{position: absolute;top: 0}
.item_img{width: 260rpx;height: 200rpx;float: left;text-align: center;font-size: 60rpx;color: #ffffff;line-height: 200rpx;}
.item_img text{font-size: 40rpx;}
.item_text{padding: 25rpx 20rpx 28rpx;float: left;font-size: 26rpx;width: 400rpx;}
.item_text_summy{padding: 5rpx 10rpx;color: #e6bf6b;border-radius: 10rpx;border: 1rpx solid #e6bf6b;width: auto;display: inline-block;margin-top: 40rpx;max-width: 350rpx;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.item_text_code{padding-top: 18rpx;overflow: hidden}
.item_text_code_left{float: left}
.item_text_code_right{float: right;}
.item_text_code_right text{color: #fe827b}
#item_0,#item_1,#item_2{position: relative;}
.not{position: absolute;width:260rpx;top: 50rpx;left: 250rpx}
.not_img{width: 180rpx;height: 180rpx;margin-left: 40rpx}
.not_font{font-size: 28rpx;text-align: center;padding-top:60rpx;color: #666666;}
.not_btn{width: 225rpx;line-height: 81rpx;text-align: center;color: #ffffff;border-radius: 6rpx;background: #E6BF6B;margin-left: 20rpx;font-size: 32rpx;margin-top: 30rpx;}