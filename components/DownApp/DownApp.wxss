.warp_bg{
   position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.3);
}
.content{
   position: fixed;
    z-index: 5000;
    width: 80%;
    max-width: 600rpx;
    height: 800rpx;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #FFFFFF;
    text-align: center;
    border-radius: 10rpx;
    overflow: hidden;
}
.content_val{
  height: 500rpx;
  position: relative;
}
.content_icon{
  width: 170rpx;
  height: 210rpx;
  position: absolute;
  top: 100rpx;
  left: 240rpx;
}
.content_font{
  font-size: 28rpx;
  color: #666666;
  text-align: center;
}
.font{
  padding-bottom: 45rpx;
}
.btn button{
  width: 520rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #FFFFFF;
  font-size: 28rpx;
  background: #000;
  text-align: center;
  border-radius: 50rpx;
}