/* pages/sharevip/sharevip.wxss */
.warp{background: #f3f3f3}
image{width: 100%;height: 100%}
.top_val{background: #ffffff;padding-bottom: 40rpx;}
.top{height: 360rpx;background: salmon;position: relative}
.top_font{position: absolute;color: #ffffff;top: 140rpx;text-align: center;font-size: 40rpx;left: 275rpx}
.top_font_bottom{font-size: 30rpx;color:rgba(212, 212, 211, 0.7)}
.top_text{padding: 40rpx 25rpx;font-size: 28rpx;}
.top_btn{width: 705rpx;margin: 0 auto;height: 90rpx;border: 1rpx solid #e6bf6b;border-radius: 15rpx;text-align: center;line-height: 90rpx;color: #e6bf6b;font-size: 28rpx;}
.item_text_top{max-height: 40px;overflow: hidden;text-overflow:ellipsis;white-space: nowrap;}
.shopList{padding: 25rpx 25rpx 0 25rpx;}
.item{height: 182rpx;overflow: hidden;background: #ffffff;margin-bottom: 20rpx;}
.item_img{width: 124rpx;height: 124rpx;padding: 30rpx 15rpx 0 20rpx;float: left}
.item_text{float: left;width: 370rpx;height: 100%;padding: 45rpx 0;box-sizing: border-box;font-size: 28rpx;}
.item_text_bottom{color: #cccccc;font-size: 26rpx;margin-top: 20rpx;overflow: hidden;text-overflow:ellipsis;white-space: nowrap;}
.item_success{width: 165rpx;height: 100%;float:left;position: relative}
.item_success_val{width: 100rpx;height: 42rpx;background: #e6bf6b;color: #ffffff;position: absolute;left: 0;top: 0;right: 0;bottom: 0;margin: auto;border-radius: 10rpx;text-align: center;line-height: 42rpx;font-size: 24rpx}
.item_success_active{width: 140rpx;height: 42rpx;color: #e6bf6b;position: absolute;left: 0;top: 0;right: 0;bottom: 0;margin: auto;border-radius: 10rpx;text-align: center;line-height: 42rpx;font-size: 24rpx;border:#e6bf6b 1rpx solid;}